<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javacode2020.chat04.mapper.UserMapper">


    <select id="getById" resultType="com.javacode2020.chat04.UserModel">

         <![CDATA[
    SELECT a.id,a.name,a.age,a.salary FROM user a WHERE a.id = #{value}
    ]]>

    </select>
</mapper>
